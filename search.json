---
layout: null
---
{% assign pages_with_titles = site.pages | where_exp: "item", "item.title != nil" %}
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "content"  : "{{ post.content | strip_html | strip_newlines | truncate: 200 | escape }}"
    },
  {% endfor %}
  {% for page in pages_with_titles %}
    {% unless page.path contains '_archived' %}
      {
        "title"    : "{{ page.title | escape }}",
        "url"      : "{{ site.baseurl }}{{ page.url }}",
        "content"  : "{{ page.content | strip_html | strip_newlines | truncate: 200 | escape }}"
      },
    {% endunless %}
  {% endfor %}
  {% for prompt in site.prompts %}
    {
      "title"    : "{{ prompt.title | escape }}",
      "url"      : "{{ site.baseurl }}/prompt-library#{{ prompt.title | slugify }}",
      "content"  : "{{ prompt.content | strip_html | strip_newlines | truncate: 200 | escape }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
